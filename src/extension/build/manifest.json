{
  "name": "oVUElord",
  "description": "Developer Tool for Vue",
  "version": "1.0",
  "permissions": [
    "contextMenus",
    "tabs",
    "activeTab"
  ],
  "host_permissions": [
    "http://localhost/*",
    "https://localhost/*"
  ],
  "manifest_version": 3,
  "minimum_chrome_version": "10.0",
  "devtools_page": "devtools.html",
  "background": {
    "service_worker": "background.bundle.js"
  },
  "content_scripts": [
    {
      "matches": [
        "http://localhost/*",
        "https://localhost/*"
      ],
      "js": [
        "content.bundle.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "script.bundle.js"
      ],
      "matches": [
        "http://localhost/*",
        "https://localhost/*"
      ]
    }
  ]
}